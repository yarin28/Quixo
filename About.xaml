<Window x:Class="Quixo.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Quixo"
        mc:Ignorable="d"
        Title="About" Height="200" Width="400">
    <Grid>
        <Label Name="Name" Content="Label" Margin="10,37,-10,-37"/>
        <Label Name="GithubLink" Content="Label" Margin="10,61,-10,-61"/>
        <Label Name="Date" Content="Label" Margin="10,92,-10,-92"/>
        <Label x:Name="about" Content="About"  FontWeight="Bold"/>

    </Grid>
</Window>
